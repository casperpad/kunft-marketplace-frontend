{
  "name": "kunft-marketplace-frontend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsnd --respawn --project tsconfig.server.json --transpile-only --watch ./server --ignore-watch=.next -r tsconfig-paths/register server/index.ts ",
    "build:next": "next build",
    "build:server": "tsc --p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "build": "pnpm run build:next && pnpm run build:server",
    "start": "set APP_ENV=production && node .",
    "lint": "next lint",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build",
    "lint:style": "npx stylelint \"**/*.{ts,tsx,scss}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:check:staged": "lint-staged",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "analyze": "ANALYZE=true yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:config": "jest src/config/__tests__/ --testPathIgnorePatterns",
    "generate-types": "mtgen ./server/models -p tsconfig.server.json -o ./server/interfaces"
  },
